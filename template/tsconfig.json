{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@i18n": ["src/i18n"],
      "@i18n/*": ["src/i18n/*"],
      "@constants": ["src/constants"],
      "@constants/*": ["src/constants/*"],
      "@config": ["src/config"],
      "@config/*": ["src/config/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@navigation": ["src/navigation"],
      "@navigation/*": ["src/navigation/*"],
      "@primitives": ["src/primitives"],
      "@primitives/*": ["src/primitives/*"],
      "@screens": ["src/screens"],
      "@screens/*": ["src/screens/*"],
      "@services": ["src/services"],
      "@services/*": ["src/services/*"],
      "@store": ["src/store"],
      "@store/*": ["src/store/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@assets": ["assets"],
      "@assets/*": ["assets/*"],
      "@icons": ["src/icons"],
      "@icons/*": ["src/icons/*"],
      "@context": ["src/context"],
      "@context/*": ["src/context/*"]
    },
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}
